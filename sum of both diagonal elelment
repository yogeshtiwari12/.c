    for(i=0;i<n;i++){
        for(j=0;j<m;j++){
    scanf("%d",&arr[i][j]);
}
    }
 

   for(i=0;i<n;i++){
        for(j=0;j<m;j++){
    printf("%d\t",arr[i][j]);
}
printf("\n");
    }


int k;

    for(i=0;i<n;i++){
        for(j=0;j<m;j++){
    if(arr[i]==arr[j]){
            
  sum += arr[i][j]; 

}
    }
    }

         for(i=0;i<m;i++){
      sum1 +=   arr[i][m-i-1];

       
     }

  

printf("sum ish = %d\n",sum);
